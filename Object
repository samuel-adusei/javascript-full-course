var samuel = {
	name: 'samuel',
	lastname: 'adusei',
	age: 22,
	yearOfBirth: 1990,
	job: 'web dev',
	isMarrired: false,
	family: ['Jane', 'Mark', 'Bob'],
	calculatedAge: function() {
		this.age = 2016 - this.yearOfBirth;
	}
};

samuel.calculatedAge();
console.log(samuel)
